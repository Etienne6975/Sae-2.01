<Window x:Class="Sae_2._01.Seconnecter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sae_2._01"
        mc:Ignorable="d"
        Title="Seconnecter" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="184,69,184,10" Width="432">
            <TextBlock Text="Identifiant" Margin="0,5" Width="62"/>
            <TextBox x:Name="txtIdentifiant" Width="200"/>
            <TextBlock Text="Mot de passe" Margin="0,10,0,5" Width="81"/>
            <PasswordBox x:Name="txtMotDePasse" Width="200"/>
            <Button Content="Se connecter" Width="120" Margin="0,20,0,0" ></Button>
        </StackPanel>
    </Grid>
</Window>
